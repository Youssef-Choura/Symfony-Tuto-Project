{% extends 'template.html.twig' %}

{% block body %}
<table class="table">
    {% for note in notes %}
     <tr>
         <td> {{ note }} </td>
     </tr>
    {% endfor %}
</table>
<div class="alert alert-info">
    <p>Nombre de notes : {{ notes | length }}</p>
    <p>Nombre minimal : {{ min(notes) }}</p>
    <p>Nombre maximal : {{ max(notes) }}</p>
</div>
{% endblock %}